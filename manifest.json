{
    "manifest_version": 2,

    "name": "DeterWebFL Plugin",
    "description": "This extension will generate deterministic WebGL rendering result",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.jpg"
    },
    "permissions": [
        "activeTab","webRequest","webRequestBlocking","downloads",
        "tabs", "http://*/*", "https://*/*","webNavigation", "*://*/*"
    ],
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["js/test.js"],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": ["js/script.js"],
    "background": {
        "scripts": ["js/jquery.min.js","js/background.js"]
    }
}
